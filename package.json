{
  "name": "proffy-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "git@github.com:mrocha98/proffy-server.git",
  "author": "Matheus Rocha <matheus_rocha98@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "start:dev": "tsnd --transpile-only --ignore-watch node_modules --respawn --no-notify src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "heroku-postbuild": "yarn tsc && yarn knex:migrate"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "http-status-codes": "^2.1.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.0.0-pre.58",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "~12.x",
    "npm": "~6.x",
    "yarn": "~1.x"
  }
}
